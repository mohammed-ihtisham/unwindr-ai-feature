{
  "name": "unwindr-interestfilter-ai",
  "version": "1.0.0",
  "description": "AI-augmented InterestFilter prototype for Unwindr using Google's Gemini API (backend-only, TypeScript).",
  "main": "dist/interestfilter-tests.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/interestfilter-tests.js",
    "dev": "ts-node --transpile-only interestfilter-tests.ts",
    "test": "npm run start",
    "test:manual": "npm run build && node -e \"require('./dist/interestfilter-tests.js').testManualTagging()\"",
    "test:llm": "npm run build && node -e \"require('./dist/interestfilter-tests.js').testLLMInference()\"",
    "test:contradiction": "npm run build && node -e \"require('./dist/interestfilter-tests.js').testContradictionHandling()\"",
    "test:slang": "npm run build && node -e \"require('./dist/interestfilter-tests.js').testSlangyDescriptions()\""
  },
  "keywords": [
    "gemini",
    "google",
    "llm",
    "unwindr",
    "interest-filter",
    "typescript",
    "backend"
  ],
  "author": "Mohammed Ihtisham",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "fs": "^0.0.1-security",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
